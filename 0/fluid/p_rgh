FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/fluid";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 1 -1 -2 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    ".*"
    {
        type            fixedFluxPressure;
        value           $internalField;
    }
    inlet
    {
    	type		zeroGradient;
    }
    fixedWallsBottom
    {
    	type		zeroGradient;
    }
    outlet
    {
        type            fixedValue;
        value           $internalField;
    }
}

